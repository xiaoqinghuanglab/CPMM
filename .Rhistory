set.seed(42)
example_df <- data.frame(
SUBID = rep(paste0("S", 1:30), each = 4),
PROCEDURE_AGE = rep(seq(60, 66, by = 2), times = 30),
ONSET_AGE = rep(sample(62:64, 30, replace = TRUE), each = 4),
CATEGORY = rep(
sample(c("Normal", "SCD", "MCI", "AD Dementia"), 30, replace = TRUE),
each = 4
),
NFL = rlnorm(120, meanlog = 2.3, sdlog = 0.35)
)
head(example_df)
groups <- list(
Normal = list(
df = example_df,
filter = rlang::expr(CATEGORY == "Normal")
),
Converters = list(
df = example_df,
filter = rlang::expr(CATEGORY %in% c("SCD", "MCI"))
),
AD = list(
df = example_df,
filter = rlang::expr(CATEGORY == "AD Dementia")
)
)
p <- plot_km_with_threshold(
biomarker_name = "NFL",
threshold = 12,
groups = groups,
time_points = seq(-6, 6, by = 2),
save = FALSE
)
