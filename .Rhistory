n_subjects <- 20
n_visits <- 6
df_status_change <- data.frame(
SUBID = rep(paste0("S", 1:n_subjects), each = n_visits),
years_since_onset = rep(seq(-6, 6, length.out = n_visits), times = n_subjects),
SEX = sample(c("M", "F"), n_subjects * n_visits, replace = TRUE),
BASELINE_AGE = rnorm(n_subjects * n_visits, mean = 65, sd = 6)
)
# Generate biologically plausible CPMM signal
df_status_change$before_onset <- pmax(0, -df_status_change$years_since_onset)
df_status_change$after_onset  <- pmax(0,  df_status_change$years_since_onset)
df_status_change$NFL <- 8 +
0.35 * df_status_change$before_onset -
0.55 * df_status_change$after_onset +
0.25 * (df_status_change$SEX == "M") +
0.02 * df_status_change$BASELINE_AGE +
rnorm(nrow(df_status_change), 0, 0.8)
plot_cpmm(
df_status_change = df_status_change,
protein = "NFL"
)
